FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
    volScalarFieldValue zoneID 123
);

X0		 -10;
Y0		 0;
Z0		 -10;

X1		 10;
Y1		 5;  //water level
Z1		 10;


regions
(
    boxToCell
    {
        box ($X0 $Y0 $Z0) ($X1 $Y1 $Z1);
        fieldValues 
        ( 
            volScalarFieldValue alpha.water 1 
        );
    }

    cellToCell
    {
        set c1;

        fieldValues
        (
            volScalarFieldValue zoneID 0
        );
    }
    cellToCell
    {
        set c2;

        fieldValues
        (
            volScalarFieldValue zoneID 1
        );
    }

);

// ************************************************************************* //
